    android:orientation = "vertical"
    >
<com.szl.zhaozhao2.view.TitlebarView
    android:id = "@+id/titleBar_CompleteCompany"
    android:layout_width = "match_parent"
    android:layout_height = "50dp"
    />

    <!-- 简称-->
    <RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textSize="15sp"
        android:paddingTop="@dimen/tv_padding_top_or_bottom"
        android:paddingBottom="@dimen/tv_padding_top_or_bottom"
        android:paddingLeft="12dp"
        android:text="简称"/>
    <EditText
        android:id="@+id/et_shorterForm_CompleteCompany"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#cccccc"
        android:textSize="15sp"
        android:paddingTop="@dimen/tv_padding_top_or_bottom"
        android:paddingBottom="@dimen/tv_padding_top_or_bottom"
        android:paddingRight="12dp"
        android:layout_alignParentRight="true"
        android:background="@null"
        android:gravity="right"
        android:singleLine="true"
        android:minWidth="100dp"
        android:hint="请输入内容"
        />
    <View
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:background="#cccccc"
        android:layout_marginRight="12dp"
        android:layout_marginLeft="12dp"
        android:layout_below="@id/et_shorterForm_CompleteCompany"
        />
</RelativeLayout>
